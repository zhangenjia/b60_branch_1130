<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RADIO">
        <attr name="radio_ruler_min_pos" format="dimension" />
        <attr name="radio_ruler_max_pos" format="dimension" />
        <attr name="radio_ruler_freq_num" format="integer" />
        <attr name="radio_ruler_text_color" format="color" />
        <attr name="radio_ruler_text_size" format="dimension" />
    </declare-styleable>
    <declare-styleable name="SeekArc">
        <attr name="radio_ska_bg_up" format="reference" />
        <attr name="radio_ska_round_bg" format="reference" />
        <attr name="radio_ska_block_up" format="reference" />
        <attr name="radio_ska_block_down" format="reference" />
        <attr name="radio_ska_max" format="integer" />
        <attr name="radio_ska_progress" format="integer" />
        <attr name="radio_ska_rotation" format="integer" />
        <attr name="radio_ska_startAngle" format="integer" />
        <attr name="radio_ska_sweepAngle" format="integer" />
        <attr name="radio_ska_block_ensable" format="boolean" />
        <attr name="radio_ska_clockwise" format="boolean" />
        <attr name="radio_ska_padding" format="dimension" />
        <attr name="radio_ska_text_color" format="color" />
        <attr name="radio_ska_text_size" format="dimension" />
        <attr name="radio_ska_backgroudspace_width" format="dimension" />
    </declare-styleable>
    <declare-styleable name="SlideMode">
        <attr name="mode">
            <enum name="forbid" value="0"></enum>
            <enum name="right" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>
